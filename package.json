{
  "name": "settings-inspector",
  "displayName": "Settings Inspector",
  "description": "A minimal, truthful inspector that reveals the real resolved values of any VS Code setting.",
  "version": "0.0.1",
  "publisher": "ekomateas",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekomateas/vscode-settings-inspector"
  },
  "keywords": [
    "settings",
    "inspector",
    "debug",
    "configuration",
    "developer-tools"
  ],
  "galleryBanner": {
    "color": "#111317",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:settingsInspector.inspect"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "settingsInspector.inspect",
        "title": "Settings Inspector: Inspect Setting"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0",
    "vsce": "^2.15.0"
  }
}